<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Crooked GitHub -->
  <remote  name="ca"
           revision="cr2"
           fetch="https://github.com/CrookedAndroid/" />

  <!-- Github -->
  <remote  name="github"
           fetch="https://github.com/" />

  <!-- Github Private -->
  <remote  name="github-priv" 
           fetch="ssh://git@github.com/" />

  <!-- GitLab -->
  <remote name="gitlab"
          fetch="https://gitlab.com/" />

  <!-- GitLab  Private -->
  <remote name="gitlab-priv" 
          fetch="ssh://git@gitlab.com" />

  <!-- Bootable -->
  <project path="bootable/recovery" name="bootable_recovery" remote="ca" />

  <!-- Build -->
  <project path="build/make" name="build_make" remote="ca" groups="pdk" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="build_soong" remote="ca" groups="pdk,tradefed" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <!-- Frameworks -->
  <project path="frameworks/base" name="frameworks_base" remote="ca" />
  <project path="frameworks/libs/systemui" name="frameworks_libs_systemui" remote="ca" />

  <!-- Hardware -->
  <project path="hardware/qcom/sdm845/display" name="hardware_qcom_sdm845_display" remote="ca" />
  <project path="hardware/qcom/sdm845/media" name="hardware_qcom_sdm845_media" remote="ca" />
  <project path="hardware/qcom/sm8150/display" name="hardware_qcom_sm8150_display" remote="ca" />

  <!-- Manifest -->
  <project path="manifest" name="manifest" remote="ca" />

  <!-- Packages -->
  <project path="packages/apps/Settings" name="packages_apps_Settings" remote="ca" />
  <project path="packages/apps/Launcher3" name="packages_apps_Launcher3" remote="ca" />
  <project path="packages/modules/Connectivity" name="packages_modules_Connectivity" remote="ca" />

  <!-- System --> 
  <project path="system/core" name="system_core" remote="ca" />
  <project path="system/libhidl" name="android_system_libhidl" remote="ca" />
	
  <!-- Vendor -->
  <project path="vendor/crooked" name="vendor_crooked" remote="ca">
    <linkfile src="build/stop_scan.mk" dest="kernel/Android.mk" />
  </project>
  <project path="vendor/pixel-framework" name="vendor_pixel-framework" remote="ca" />

  <!-- Pixel 3/XL Stuff -->
  <project name="device_google_crosshatch" path="device/google/crosshatch" remote="ca" />
  <project name="device_google_crosshatch-sepolicy" path="device/google/crosshatch-sepolicy" remote="ca" />
<!--  <project name="vendor_google_crosshatch" path="vendor/google/crosshatch" remote="ca" /> -->

  <!-- Arrow vendor for crosshatch -->
  <project name="ArrowOS-Devices/android_vendor_google_bluecross" path="vendor/google" remote="github" revision="arrow-13.0" />
  <!-- Arrow Gapps -->
  <project name="arrowos-project/android_vendor_gapps" path="vendor/gapps" remote="gitlab" revision="arrow-13.0" />

  <project name="CrookedAndroid/device_google_crosshatch-kernel" path="device/google/crosshatch-kernel" remote="gitlab" revision="cr2" />

<!--  <remove-project name="android_packages_apps_Statix_Wallpapers" remote="statix" /> -->
  <project name="neporood/packages_apps_CrookedWalls" path="packages/apps/CrookedWalls" remote="github-priv" revision="stg/cr2" />

  <remove-project name="platform/packages/apps/Camera2" />

  <project name="CrookedAndroid/android_vendor_google_pixelparts" path="vendor/google/pixelparts" remote="gitlab" revision="cr2" />

  <project name="ProtonAOSP/android_packages_apps_ElmyraService" path="packages/apps/ElmyraService" remote="github" revision="sc-qpr3" />

</manifest>
